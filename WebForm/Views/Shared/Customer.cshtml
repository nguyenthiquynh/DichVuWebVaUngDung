<body>
    <div class="wrapper">
        <!-- ============================================================= TOP NAVIGATION ============================================================= -->
        <nav class="top-bar animate-dropdown">
            <div class="container">
                <div class="col-xs-12 col-sm-6 no-margin">
                    <ul>
                        <li><a class="dropdown-toggle" data-toggle="dropdown" href="~/View/Home/Index">Trang chủ</a></li>
                        <li><a href="/Home/SearchAdvanced">Tìm kiếm nâng cao</a></li>
                    </ul>
                </div><!-- /.col -->
                <div class="col-xs-12 col-sm-6 no-margin">
                    <ul class="right">
                        @if (Session["LogedUserFullname"] != null)
                        {
                            <li class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" href="#change-language">@Session["LogedUserFullname"] &nbsp;</a>
                                <ul class="dropdown-menu" role="menu">
                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Thông tin cá nhân</a></li>
                                    @if (Session["LogedUserFullname"] != null && int.Parse(Session["LogedUserRule"].ToString()) >0)
                                    {
                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="/Home/History">Lịch sử mua hàng</a></li>
                                     }  
                                    @if (Session["LogedUserFullname"] != null && int.Parse(Session["LogedUserRule"].ToString()) == 0)
                                    {
                                        <li><a href="/Admin/Index">Quản lý </a></li>
                                    }                          
                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="/Home/Logout">Đăng xuất</a></li>
                                </ul>
                            </li>
                        }
                        <li>
                            <a href="#">Yêu thích(0)</a>
                        </li>
                        @if (Session["LogedUserFullname"] == null)
                        {
                            <li><a href="/Home/Register">Đăng Kí</a></li>
                            <li><a href="/Home/Authentication">Đăng Nhập</a></li>
                        }
                    </ul>
                </div><!-- /.col -->
            </div><!-- /.container -->
        </nav>
        <!-- ============================================================= TOP NAVIGATION : END ============================================================= -->
        <!-- ============================================================= HEADER ============================================================= -->
        <header class="no-padding-bottom header-alt">
            <div class="container no-padding" style="height:160px">

                <div class="col-xs-12 col-md-3 logo-holder">
                    <!-- ============================================================= LOGO ============================================================= -->
                    <div class="logo">
                        <a href="/">
                            <img src="~/IMAGE/1367830288-563c5f3b138ea37db320fd5695f6f6c4.png" alt="logo" style="width:200px;height:180px;margin-left:30px;margin-top:-35px"></a>
                    </div><!-- /.logo -->
                    <!-- ============================================================= LOGO : END ============================================================= -->
                </div><!-- /.logo-holder -->
                <div class="col-xs-12 col-md-6 top-search-holder no-margin" >
                    <div class="contact-row">
                        <div class="phone inline">
                            <i class="fa fa-phone"></i> (+08) 38 38 96 69
                        </div>
                        <div class="contact inline">
                            <i class="fa fa-envelope"></i> Liên hệ &nbsp;<span class="le-color">dinh.phuonghao7620@gmail.com</span>
                        </div>
                    </div><!-- /.contact-row -->
                    <!-- ============================================================= SEARCH AREA ============================================================= -->
                    <div class="search-area">
                        @using (Html.BeginForm("Search", "Home", FormMethod.Get))
                        {
                            <div class="control-group">
                                @Html.TextBox("searchString", ViewBag.CurrentFilter as string, new { @class = "search-field", placeholder = "Nhập tên sản phẩm..."})
                                
                                <button type="submit" class="search-button"></button>                                
                            </div>
                            
                        }
                        
                    </div><!-- /.search-area -->
                    <!-- ============================================================= SEARCH AREA : END ============================================================= -->
                </div><!-- /.top-search-holder -->
                <div class="col-xs-12 col-md-3 top-cart-row no-margin" >
                    <div class="top-cart-row-container">

                        <!-- ============================================================= SHOPPING CART DROPDOWN ============================================================= -->
                        <div class="top-cart-holder dropdown animate-dropdown">

                            <div class="basket">

                                <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                    <div class="basket-item-count">
                                        <span class="count">$</span>
                                        <img src="~/Content/Customer/css/images/icon-cart.png" alt="" />
                                    </div>
                                    <div class="total-price-basket">
                                        <span class="lbl">Giỏ hàng</span>
                                        <span class="total-price"></span>
                                    </div>
                                </a>
                                <ul class="dropdown-menu">
                                    <li class="checkout">
                                        <div class="basket-item">
                                            <div class="row">
                                                <div class="col-xs-12 col-sm-6">
                                                    <a href="/Cart/Index" class="le-button inverse"> Giỏhàng </a>
                                                </div>
                                                <div class="col-xs-12 col-sm-6">
                                                    <a href="/Cart/Index" class="le-button">Thanhtoán</a>
                                                </div>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </div><!-- /.basket -->
                        </div><!-- /.top-cart-holder -->
                    </div><!-- /.top-cart-row-container -->
                    <!-- ============================================================= SHOPPING CART DROPDOWN : END ============================================================= -->
                </div><!-- /.top-cart-row -->
            </div><!-- /.container -->
            <!-- ========================================= NAVIGATION ========================================= -->
            <nav id="top-megamenu-nav" class="megamenu-vertical animate-dropdown">
                <div class="container">
                    <div class="yamm navbar">
                        <div class="navbar-header">
