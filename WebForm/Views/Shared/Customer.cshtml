    <link href="~/Content/Customer/css/animate.min.css" rel="stylesheet" />
    <link href="~/Content/Customer/css/green.css" rel="stylesheet" />
    <link href="~/Content/Customer/css/main.css" rel="stylesheet" />
    <link href="~/Content/Customer/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/Customer/css/owl.transitions.css" rel="stylesheet" />

    <!-- Demo Purpose Only. Should be removed in production -->
    <link href="~/Content/Customer/css/config.css" rel="alternate stylesheet" />
    <link href="~/Content/Customer/css/green.css" rel="alternate stylesheet" title="Green color" />
    <link href="~/Content/Customer/css/blue.css" rel="alternate stylesheet" title="Blue color" />
    <link href="~/Content/Customer/css/navy.css" rel="alternate stylesheet" title="Navy color" />
